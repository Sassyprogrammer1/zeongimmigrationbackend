{
  "name": "zeongimmigrationbackend",
  "version": "1.0.0",
  "description": "Backend Apis and serverless infra",
  "main": "index.js",
  "scripts": {
    "predev": "npm run swagger",
    "prebuild": "npm run swagger",
    "build": "tsc",
    "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec\"",
    "swagger": "tsoa spec",
    "start:dev": "nodemon",
    "start:prod": "webpack --mode production",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sassyprogrammer1/zeongimmigrationbackend.git"
  },
  "nodemonConfig": {
    "delay": 1000,
    "watch": [
      "src"
    ],
    "ext": "ts,json",
    "ignore": [
      "src/**/*.spec.ts"
    ],
    "exec": "ts-node ./src/index.ts"
  },
  "author": "Hamza Jamshed",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Sassyprogrammer1/zeongimmigrationbackend/issues"
  },
  "homepage": "https://github.com/Sassyprogrammer1/zeongimmigrationbackend#readme",
  "devDependencies": {
    "@types/aws-serverless-express": "^3.3.5",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.9",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/webpack-dev-server": "^4.7.2",
    "babel-loader": "^9.1.0",
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20",
    "rimraf": "^3.0.2",
    "serverless-dotenv-plugin": "^4.0.2",
    "serverless-plugin-typescript": "^2.1.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@typedorm/common": "^1.15.3",
    "@typedorm/core": "^1.15.3",
    "@types/body-parser": "^1.19.2",
    "aws-sdk": "^2.1277.0",
    "aws-serverless-express": "^3.4.0",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "serverless-http": "^3.1.0",
    "swagger-ui-express": "^4.6.0",
    "tsoa": "^4.1.3"
  }
}
