// 'localhost' doesn't work here, use your local IP instead.
FRONTEND_URL_DOMAIN=null|[YOUR URL]
REDSYS_SIGNATURE=
REDSYS_URL=
SMTP_SERVER=
SMTP_USER=
SMTP_PASSWORD=
SMTP_PORT=
GMAPS_API_KEY=
PROJECT_ID=
BUCKET_NAME=
CRON_JOB_API_KEY=
